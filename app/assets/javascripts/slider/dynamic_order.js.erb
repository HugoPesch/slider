$(document).ready(function(){
  /*gallerySelected();*/

  $('#gallery_uploads').on('change', function(){
    id = $('#gallery_uploads').val();
    ids = "";
    $(id).each (function(index, imgId){
      ids += "id[]="+imgId+"&";
    });
    console.log(ids);
    console.log(id);
    if (id != "" ){
      $.ajax({
        type: 'POST',
        url: "/uploads/order",
        data: ids,
        beforeSend: function() {
        },
        success:function(data){
              console.log(data);
              $('#order').html(data);
         }
      })
    }
  });
});
